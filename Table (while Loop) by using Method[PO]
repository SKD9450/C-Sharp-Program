using System;
class Table
{
    static void number(int num)
    {
        int i = 1;
        while (i <= 10)
        {
            Console.WriteLine("{0}*{1}={2}", num, i, num * i);
            i++;
        }
    }
    static void Main()
    {
        Console.Write("Enter the number:");
        int num = int.Parse(Console.ReadLine());
        number(num);
        Console.ReadKey();
    }
}
