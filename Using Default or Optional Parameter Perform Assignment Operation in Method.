class demo
{
    static string cal(int x,int y,string choice = "+")
    {
        string res = " ";
        if (choice == "+")
            res = System.Convert.ToString(x + y);
       else if (choice == "-")
            res = System.Convert.ToString(x - y);
        else if (choice == "*")
            res = System.Convert.ToString(x * y);

        else if (choice == "/")
            res = System.Convert.ToString(x / y);
        return res;

    }
    static void Main()
    {
        string a = cal(10, 2);
        System.Console.WriteLine("Res is :" + a);
        System.Console.ReadKey();
    }
}
