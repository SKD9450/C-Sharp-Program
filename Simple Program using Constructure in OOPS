using System;
class test
{
    int x;
    static int y;
    public void Show()
    {
        x = x + 1;
        y = y + 1;
        Console.WriteLine(x + " " + y);
    }
}
class demo
{
    static void Main()
    {
        test t1 = new test();
        test t2 = new test();
        test t3 = new test();
        t1.Show();
        t2.Show();
        t3.Show();
    }
}
